{
  "name": "bridgecmdr",
  "productName": "BridgeCmdr",
  "version": "2.0.0-beta",
  "description": "Professional switch and monitor controller for retro gaming",
  "main": "./dist/main/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "production": "npm run prod",
    "prod": "cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.config.js",
    "devevlopment": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --config build/webpack.config.js",
    "start": "electron .",
    "test": "test.js",
    "package": "electron-builder --linux snap",
    "package:arm": "electron-builder --armv7l --linux snap",
    "package:arm64": "electron-builder --arm64 --linux snap",
    "package:dir": "electron-builder --dir",
    "dist": "npm run prod && npm run package",
    "dist:arm": "npm run prod && npm run package:arm",
    "dist:arm64": "npm run prod && npm run package:arm64",
    "pack": "npm run prod && npm run package:dir"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/6XGate/bridgecmdr.git"
  },
  "keywords": [
    "linux",
    "raspberry-pi",
    "retrogaming",
    "audio-video",
    "sony-bmv",
    "sony-pvm",
    "extron"
  ],
  "author": {
    "name": "Matthew Holder"
  },
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/6XGate/bridgecmdr/issues"
  },
  "homepage": "https://github.com/6XGate/bridgecmdr#readme",
  "build": {
    "appId": "org.sleepingcats.bridgecmdr",
    "productName": "BridgeCmdr",
    "copyright": "Copyright 2019-2020 Matthew Holder",
    "buildDependenciesFromSource": true,
    "linux": {
      "category": "AudioVideo"
    },
    "files": [
      "dist/main/**/*",
      "dist/render/**/*"
    ],
    "snap": {
      "summary": "Controls monitors and A/V equipment in a retrogaming setup",
      "confinement": "classic",
      "grade": "devel"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@mdi/font": "^5.3.45",
    "@mdi/js": "^5.3.45",
    "@mdi/svg": "^5.3.45",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/lodash": "^4.14.157",
    "@types/pouchdb": "^6.4.0",
    "@types/serialport": "^8.0.1",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "buefy": "^0.8.20",
    "bulma": "^0.9.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "electron": "^8.4.0",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-unhandled": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "fibers": "^5.0.0",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.3.0",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^0.9.0",
    "pouchdb-browser": "^7.2.1",
    "pouchdb-find": "^7.2.1",
    "read-package-json": "^2.1.1",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "type-fest": "^0.15.1",
    "typescript": "^3.9.6",
    "url-loader": "^4.1.0",
    "uuid": "^8.2.0",
    "vee-validate": "^3.3.7",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.3.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vue-tsx-support": "^2.3.3",
    "vuex": "^3.5.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "serialport": "^9.0.0"
  }
}
